 <% flash.each do |type, msg| %>
      <% if type == "notice" %>
        <div class="alert alert-success mt-1">
         <i class="bi bi-check-square-fill h3"></i>
          <%= msg %>
        </div>
      <% else %>
        <div class="alert alert-danger mt-1">
          <i class="bi bi-exclamation-triangle-fill h3"></i>
          <%= msg %>
        </div>
      <% end %>
    <% end %>